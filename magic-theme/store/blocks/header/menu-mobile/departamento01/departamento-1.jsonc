{
  "drawer#dep1-mobile-1": {
    "title": "Panda | Menu Departamento 01 - Nivel 02",
    "children": [
      "flex-layout.row#dep1-1-mobile-nivel-2-head",
      "vtex.menu@2.x:menu#dep1-mobile-nivel-3",
      "link#dep1-1-mobile-nivel-2",
      "flex-layout.row#dep1-1-mobile-nivel-1-footer"
    ],
    "blocks": [
      "drawer-header#header-mobile",
      "drawer-trigger#dep1-mobile"
    ],
    "props": {
      "blockClass": "menu-mobile-nivel01"
    }
  },

  "flex-layout.row#dep1-1-mobile-nivel-2-head": 
  {
    "title": "Panda | Menu Departamento 01 - Nivel 02 - Header",
    "props": {
      "blockClass": "header__nivel3--mob",
      "colSizing": "auto",
      "colJustify": "left",
      "colGap": 4
    },
    "children": ["drawer-close-button", "rich-text#dep1-1-mobile-nivel-2-head"]
  },

  "rich-text#dep1-1-mobile-nivel-2-head": {
    "title": "Panda | Menu Departamento 01 - Nivel 2 - Header Titulo",
    "props": {
      "text": "lançamentos",
      "blockClass": "header__nivel3--mob"
    }
  },
  
  "drawer-trigger#dep1-mobile": {
    "children": ["rich-text#dep1-mobile"],
    "props": {
      "blockClass": "menu-mobile-nivel01"
    }
  },
  
  "rich-text#dep1-mobile": {
    "title": "Panda | Menu Departamento 01 - Nivel 02 - Texto",
    "props": {
      "text": "lançamentos",
      "blockClass": "menu-mobile-nivel01"
    }
  },


  "vtex.menu@2.x:menu#dep1-mobile-nivel-3": {
    "title": "Panda | Menu Departamento 01 - Nivel 02 - Submenu",
    "props": {
      "blockClass": ["menu-mobile-wrapper"],
      "orientation": "vertical",
      "experimentalOptimizeRendering": true
    },
    "children": [
      "drawer#dep1-1-mobile-nivel-3",
      "drawer#dep1-2-mobile-nivel-3",
      "drawer#dep1-3-mobile-nivel-3"
    ]
  },

  "link#dep1-1-mobile-nivel-2": {
    "title": "Panda | Menu Departamento 01 - Nivel 02 - Ver tudo",
    "props": {
      "label": "todos os lançamentos",
      "href": "#",
      "blockClass": ["menu-mobile-nivel01", "menu-see-all-mobile"]
    }
  }
}
