  "rich-text#itemBeneficios--blackFrida03": {
    "title": "Panda | Item slider beneficios Black Friday 03",
    "props": {
      "text": "5% off no pix",
      "blockClass": "itemBeneficios--blackFrida03"
    }
  },

  /* Mobile */
  "slider-layout#sliderBeneficios-blackFriday-Mob": {
    "title": "Panda | Itens slider beneficios Black Friday - Mobile",
    "props": {
      "itemsPerPage": {
