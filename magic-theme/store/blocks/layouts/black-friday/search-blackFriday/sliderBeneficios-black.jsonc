{
  "experimental__visibility-layout#sliderBeneficios-blackFriday": {
    "title": "Panda | Controle de visibilidade - Slider beneficios Black Friday",
    "children": ["flex-layout.row#sliderBeneficios-blackFriday"],
    "props": {
      "visible": true,
      "blockClass": "sliderBeneficios-blackFriday"
    }
  },

  "flex-layout.row#sliderBeneficios-blackFriday": {
    "title": "Panda | Slider beneficios Black Friday",
    "children": [
      "responsive-layout.desktop#sliderBeneficios-blackFriday",
      "responsive-layout.phone#sliderBeneficios-blackFriday"
    ],
    "props": {
      "blockClass": "sliderBeneficios-blackFriday",
      "fullWidth": true,
      "preventVerticalStretch": true,
      "preventHorizontalStretch": true
    }
  },

  "responsive-layout.desktop#sliderBeneficios-blackFriday": {
    "children": ["slider-layout#sliderBeneficios-blackFriday"]
  },

  "responsive-layout.phone#sliderBeneficios-blackFriday": {
    "children": ["slider-layout#sliderBeneficios-blackFriday-Mob"]
  },

  /* Desktop */
  "slider-layout#sliderBeneficios-blackFriday": {
    "title": "Panda | Itens slider beneficios Black Friday",
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 3,
        "phone": 3
      },
      "infinite": false,
      "showNavigationArrows": "never",
      "showPaginationDots": "mobileOnly",
      "centerModeSlidesGap": 31,
      "centerMode": {
        "desktop": "disabled",
        "tablet": "disabled",
        "phone": "to-the-left"
      },
      "blockClass": "sliderBeneficios-blackFriday"
    },
    "children": [
      "rich-text#itemBeneficios--blackFrida01",
      "rich-text#itemBeneficios--blackFrida03",
      "rich-text#itemBeneficios--blackFrida02"
    ]
  },
  "rich-text#itemBeneficios--blackFrida01": {
    "title": "Panda | Item slider beneficios Black Friday 01",
    "props": {
      "text": "[frete grátis confira as regras](https://www.magicfeet.com.br/tipos-de-entrega)",
