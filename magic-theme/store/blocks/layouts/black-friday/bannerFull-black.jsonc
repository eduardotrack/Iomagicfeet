      "responsive-layout.mobile#blackFriday--banner-Mob"
    ],
    "props": {
      "visible": true,
      "blockClass": "blackFriday--banner"
    }
  },

  "responsive-layout.desktop#blackFriday--banner-Desk": {
    "title": "Panda | Banner Black friday - Desktop",
    "children": ["flex-layout.row#blackFriday--banner-Desk"]
